  "main": "index.js",
  "packageManager": "bun@1.2.19",
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "nx affected --target=lint --parallel=3",
    "lint:fix": "nx affected --target=lint --fix --parallel=3",
    "typecheck": "nx affected --target=typecheck --parallel=3",
    "test": "nx affected --target=test --parallel=3",
    "test:all": "nx run-many --target=test --all",
    "test:coverage": "nx affected --target=test --coverage --parallel=3",
    "build": "nx affected --target=build --parallel=3",
    "build:all": "nx run-many --target=build --all",
    "affected": "nx affected:graph",
    "graph": "nx graph",
    "check-all": "bun run format:check && bun run lint && bun run typecheck && bun run test"
  },
