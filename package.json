{
  "name": "ftry",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "bun@1.2.19",
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "nx affected --target=lint --parallel=3",
    "lint:fix": "nx affected --target=lint --fix --parallel=3",
    "typecheck": "nx affected --target=typecheck --parallel=3",
    "test": "nx affected --target=test --parallel=3",
    "test:all": "nx run-many --target=test --all",
    "test:coverage": "nx affected --target=test --coverage --parallel=3",
    "build": "nx affected --target=build --parallel=3",
    "build:all": "nx run-many --target=build --all",
    "affected": "nx affected:graph",
    "graph": "nx graph",
    "check-all": "bun run format && concurrently --kill-others-on-fail --names \"lint,types,test\" \"bun run lint\" \"bun run typecheck\" \"bun run test\"",
    "env": "docker compose up -d",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:create": "bunx prisma migrate dev --create-only",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:push": "bunx prisma db push",
    "db:seed": "bunx prisma db seed",
    "db:reset": "bunx prisma migrate reset",
    "db:studio": "bunx prisma studio"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anshulbisen/ftry.git"
