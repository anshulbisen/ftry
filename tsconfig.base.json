{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@ftry/backend/auth": ["libs/backend/auth/src/index.ts"],
      "@ftry/backend/cache": ["libs/backend/cache/src/index.ts"],
      "@ftry/backend/common": ["libs/backend/common/src/index.ts"],
      "@ftry/backend/health": ["libs/backend/health/src/index.ts"],
      "@ftry/backend/logger": ["libs/backend/logger/src/index.ts"],
      "@ftry/backend/monitoring": ["libs/backend/monitoring/src/index.ts"],
      "@ftry/backend/queue": ["libs/backend/queue/src/index.ts"],
      "@ftry/backend/redis": ["libs/backend/redis/src/index.ts"],
      "@ftry/shared/constants": ["libs/shared/constants/src/index.ts"],
      "@ftry/shared/prisma": ["libs/shared/prisma/src/index.ts"],
      "@ftry/shared/types": ["libs/shared/types/src/index.ts"],
      "@ftry/shared/util-formatters": ["libs/shared/util-formatters/src/index.ts"],
      "@ftry/shared/utils": ["libs/shared/utils/src/index.ts"],
      "admin": ["libs/backend/admin/src/index.ts"],
      "monitoring": ["libs/backend/monitoring/src/index.ts"],
      "redis": ["libs/backend/redis/src/index.ts"],
      "util-encryption": ["libs/shared/util-encryption/src/index.ts"],
      "util-types": ["libs/shared/util-types/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
